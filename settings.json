{
  "editor.tabCompletion": "onlySnippets",
  "editor.snippetSuggestions": "top",
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#27bee8ef",
    "scrollbarSlider.activeBackground": "#8c3ce2ca"
  },
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.color": "#a3ffbf",
  "editor.fontFamily": "\"SF Mono Ligatures\"",
  "editor.fontLigatures": true,
  "workbench.editor.wrapTabs": true,
  "editor.wordWrap": "on",

  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": false
  },
  "indentRainbow.colors": [
    "rgb(153, 221, 204, 0.025);",
    "rgb(255, 93, 162,0.025);",
    "rgb(156, 25, 224,0.025);)",
    "rgb(0, 13, 107,0.025);"
  ],
  "editor.tokenColorCustomizations": {},
  "editor.lineHeight": 2,
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.colorTheme": "Evondev Dracula Normal Constrast - No Highlight",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "javascript.updateImportsOnFileMove.enabled": "always",

  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "editor.suggestSelection": "first",
  "editor.wordWrapColumn": 100,
  "powermode.combo.timeout": 0,
  "powermode.shake.enabled": false,
  "powermode.explosions.size": 5,
  "powermode.combo.counterEnabled": "hide",
  "powermode.enabled": true,
  "powermode.combo.counterSize": 2,
  "color-highlight.markerType": "underline",
  "git.autofetch": true,
  "git.confirmSync": false,
  "editor.accessibilitySupport": "off",
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "php.validate.executablePath": "C:/xampp/php/php.exe",
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "files.associations": {
    "*.php": "php"
  },
  "npm.keybindingsChangedWarningShown": true,
  "tabnine.experimentalAutoImports": true,
  "window.zoomLevel": 0.5,
  "editor.fontSize": 15,
  "vscode_custom_css.imports": [
    "file:///C:/Users/ASUS/VSCode_Extension/colorsText.css"
  ]
}
